{
  "name": "admin-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "commit": "cz",
    "lint": "prettier --write .",
    "commitlint": "commitlint --config commitlint.config.ts -e -V",
    "prepare": "husky"
  },
  "dependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@commitlint/types": "^20.2.0",
    "@release-it/conventional-changelog": "^10.0.4",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "release-it": "^19.2.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css,scss,vue}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "cz-git"
    }
  }
}
